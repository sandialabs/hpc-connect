#!/bin/sh
{% block header required %}{% endblock %}
{% block environment %}
{% if job.variables %}
  {% for var, val in job.variables.items() %}
    {% if val is none %}
unset {{ var }}
    {% else %}
export {{ var }}={{ val }}
    {% endif %}
  {% endfor %}
{% endif %}
printenv || true
{% endblock %}
{% block commands %}
{% for command in job.commands %}
{{ command }}
{% endfor %}
{% endblock %}
